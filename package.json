{
    "name": "react-css-loading",
    "version": "1.0.0",
    "description": "CSS loading spinner for React.",
    "main": "dist/bundle.cjs.js",
    "module": "dist/bundle.esm.js",
    "sideEffects": false,
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "rollup -c",
        "lint": "eslint \"**/*.js\"",
        "lint:fix": "prettier-eslint \"**/*.js\" --write",
        "storybook": "start-storybook -p 9001 -c storybook"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "npm run lint:fix",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/philwamba/react-css-loading.git"
    },
    "keywords": [
        "react",
        "css",
        "loading"
    ],
    "author": "Phil Wamba",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/philwamba/react-css-loading/issues"
    },
    "homepage": "https://github.com/philwamba/react-css-loading#readme",
    "devDependencies": {
        "@babel/core": "^7.6.2",
        "@babel/preset-env": "^7.6.2",
        "@babel/preset-react": "^7.0.0",
        "@storybook/react": "^6.5.14",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.0.6",
        "eslint": "^6.4.0",
        "eslint-config-standard": "^14.1.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jest": "^22.17.0",
        "eslint-plugin-node": "^10.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.14.3",
        "eslint-plugin-standard": "^4.0.1",
        "husky": "^3.0.7",
        "jest": "^24.9.0",
        "lint-staged": "^9.4.0",
        "prettier-eslint": "^9.0.0",
        "prettier-eslint-cli": "^5.0.0",
        "react": "^16.10.1",
        "react-dom": "^16.10.1",
        "react-test-renderer": "^16.10.1",
        "rimraf": "^3.0.0",
        "rollup": "^1.21.4",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-string": "^3.0.0",
        "rollup-plugin-terser": "^5.1.2"
    },
    "peerDependencies": {
        "react": ">=15"
    },
    "engines": {
        "node": ">=8"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.css$": "<rootDir>/__mocks__/styles.js"
        }
    }
}
